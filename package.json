    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "css-loader": "^0.26.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "global": "^4.3.1",
    "json-loader": "^0.5.4",
    "postcss-loader": "^1.2.0",
    "react": "15.4.1",
    "react-dom": "^15.4.1",
    "react-native": "0.39.1",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
